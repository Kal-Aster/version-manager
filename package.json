{
  "name": "version-manager",
  "displayName": "Version manager",
  "description": "This vscode extension allow defining changes and manage versioning",
  "version": "0.4.1",
  "repository": {
    "url": "https://github.com/Kal-Aster/version-manager"
  },
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:version-manager-sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "version-manager-sidebar-view",
          "title": "Version manager",
          "icon": "media/checklist.svg"
        }
      ]
    },
    "views": {
      "version-manager-sidebar-view": [
        {
          "type": "webview",
          "id": "version-manager-sidebar",
          "name": "Version manager",
          "icon": "media/checklist.svg",
          "contextualTitle": "Version manager"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --config ./build/node-extension.webpack.config.js",
    "watch": "concurrently \"rollup -c -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-typescript": "^8.3.0",
    "@tsconfig/svelte": "^1.0.13",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.9",
    "@types/polka": "^0.5.3",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "concurrently": "^6.4.0",
    "eslint": "^8.3.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "rollup": "^2.60.1",
    "rollup-plugin-svelte": "^6.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^2.0.4",
    "svelte": "^3.44.2",
    "svelte-check": "^1.6.0",
    "svelte-preprocess": "^4.9.8",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.2",
    "vsce": "^2.4.0",
    "vscode-test": "^1.6.1",
    "webpack": "^5.64.2",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@vscode/codicons": "^0.0.26"
  }
}